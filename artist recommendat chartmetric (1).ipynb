    "        print(\"No similar artists found to plot.\")\n",
    "        return\n",
    "    names = [artist['name'] for artist in similar_artists]\n",
    "    scores = [artist.get('sp_popularity', 0) for artist in similar_artists]\n",
    "\n",
    "    # Normalize scores for color mapping (higher = more similar)\n",
    "    norm = plt.Normalize(min(scores), max(scores))\n",
    "    # Use a blue-to-red colormap (Blues reversed to Reds)\n",
    "    cmap = plt.cm.get_cmap('RdYlBu_r')  # 'RdYlBu_r' is blue for high, red for low\n",
    "\n",
    "    colors = cmap(norm(scores))\n",
    "\n",
    "    plt.figure(figsize=(10, 6))\n",
    "    bars = plt.barh(names, scores, color=colors)\n",
    "    plt.xlabel('Spotify Popularity')\n",
    "    plt.title(f'Top Similar Artists to {main_artist_name}')\n",
    "    plt.gca().invert_yaxis()\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "\n",
    "def find_artist_id(artist_name, artists):\n",
    "    artist_name = artist_name.strip().lower()\n",
    "    # Exact match\n",
    "    for artist in artists:\n",
    "        if artist_name == artist['label'].lower():\n",
    "            return artist['id'], artist['label']\n",
    "    # Partial match\n",
    "    for artist in artists:\n",
    "        if artist_name in artist['label'].lower():\n",
    "            return artist['id'], artist['label']\n",
    "    return None, None\n",
    "\n",
    "def main():\n",
    "    artist_input = input(\"Enter artist name: \").strip()\n",
    "    artist_id, artist_label = find_artist_id(artist_input, ARTISTS)\n",
    "    if artist_id is None:\n",
    "        print(f\"Artist '{artist_input}' not found in the artist list.\")\n",
    "        return\n",
    "\n",
    "    print(f\"Selected artist: {artist_label} (ID: {artist_id})\")\n",
    "\n",
    "    print(\"Refreshing Chartmetric access token...\")\n",
    "    access_token = refresh_chartmetric_access_token()\n",
    "\n",
    "    print(f\"Fetching similar artists for {artist_label}...\")\n",
    "    similar_artists = get_neighboring_artists(artist_id, access_token, limit=10)\n",
    "\n",
    "    print(f\"\\nTop 10 Similar Artists to {artist_label}:\")\n",
    "    print(f\"{'Rank':<5} {'Artist':<35} {'Spotify Popularity':<18} {'Chartmetric ID':<15}\")\n",
    "    print(\"-\" * 80)\n",
    "    if not similar_artists:\n",
    "        print(\"No similar artists found.\")\n",
    "    else:\n",
    "        for i, artist in enumerate(similar_artists, 1):\n",
    "            print(f\"{i:<5} {artist['name']:<35} {artist.get('sp_popularity', 'N/A'):<18} {artist['id']:<15}\")\n",
    "\n",
    "    plot_similarity_ranking(similar_artists, artist_label)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n"
   ]
